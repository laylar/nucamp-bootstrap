{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "This is a website for fictional campsites review site Nucamp",
  "main": "index.js",
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "Layla Rowen",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.3.0",
    "node-sass": "^4.12.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laylar/nucamp-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/laylar/nucamp-bootstrap/issues"
  },
  "homepage": "https://github.com/laylar/nucamp-bootstrap#readme"
}
